import vizualizacao.EntradaSaida;

public class controladora {

	public void exibeMenu() {
		
		int opcoes;
		
		do {
			opcoes = dados.opcoesPerguntas();

			switch(opcoes) {
				case 0:
					String cor = EntradaSaida.solicitaCor("cor");
					String modelo = EntradaSaida.solicitaModelo("modelo");
					
					int ano = ("ano");
					
			}
			
		} while (opcoes != 3);
	}
	
}
